   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "from scipy.stats import pearsonr, chi2_contingency\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from IPython.display import display, HTML\n",
    "\n",
    "#to make the output look nicer\n",
    "np.set_printoptions(suppress=True, precision = 2)\n",
    "\n",
    "nba = pd.read_csv('nba_games.csv')\n",
    "\n",
    "\n",
    "# Subset Data to 2010 Season, 2014 Season\n",
    "nba_2010 = nba[nba.year_id == 2010]\n",
    "nba_2014 = nba[nba.year_id == 2014]\n",
    "knks_pts = nba_2010[nba_2010.fran_id == 'Knicks']['pts']\n",
    "nets_pts = nba_2010[nba_2010.fran_id == 'Nets']['pts']\n",
    "knks_ave = np.mean(knks_pts)\n",
    "nets_ave = np.mean(nets_pts)\n",
    "pts_diff_2010 = knks_ave - nets_ave \n",
    "\n",
    "display(HTML(nba.head(15).to_html()))\n",
    "plt.hist(knks_pts, alpha=0.8, density=True, label='knicks')\n",
    "plt.hist(nets_pts, alpha=0.8, density=True, label='nets')\n",
    "plt.legend()\n",
    "plt.title(\"2010 Season\")\n",
    "plt.show()\n",
    "#compare the differance from 2010 - 2014\n",
    "knks_pts_2014 = nba_2014[nba_2014.fran_id == 'Knicks']['pts']\n",
    "nets_pts_2014 = nba_2014[nba_2014.fran_id == 'Nets']['pts']\n",
    "mean_knks_2014 = np.mean(knks_pts_2014)\n",
    "mean_nets_2014 = np.mean(nets_pts_2014)\n",
    "pts_diff_2014 = mean_knks_2014 - mean_nets_2014\n",
    "\n",
    "plt.hist(knks_pts_2014, alpha=0.8, density=True, label='knicks')\n",
    "plt.hist(nets_pts_2014, alpha=0.8, density=True, label='nets')\n",
    "plt.legend()\n",
    "plt.title(\"2014 Season\")\n",
    "plt.show()\n",
    "\n",
    "#I just learnied that the larger the average number differance is that there is a larger \"disparity\" between the two series, and that the smaller the closer they are on average.\n",
    "# print(pts_diff_2014)\n",
    "# print(pts_diff_2010)\n",
    "# This next portion im only going to be dealing with the 2010 dataset for now:\n",
    "plt.close()\n",
    "sns.boxplot(data=nba_2010, x='fran_id', y='pts')\n",
    "plt.title(\"Points Scored by Teams in 2010\")\n",
    "plt.show()\n",
    "\n",
    "\n",
    "location_result_freq = pd.crosstab(nba_2010['game_result'], nba_2010['game_location'])\n",
    "print(location_result_freq)\n",
    "\n",
    "location_result_proportions = location_result_freq / len(nba_2010)\n",
    "print(location_result_proportions)\n",
    "\n",
    "\n",
    "chi2, pval, dof, expected = chi2_contingency(location_result_freq)\n",
    "print(expected)\n",
    "print(chi2)\n",
    "\n",
    "\n",
    "# Calculate the covariance matrix\n",
    "cov_matrix = np.cov(nba_2010['forecast'], nba_2010['point_diff'])\n",
    "\n",
    "# Extract the covariance value\n",
    "point_diff_forecast_cov = cov_matrix[0, 1]\n",
    "print(point_diff_forecast_cov)\n",
    "plt.close()\n",
    "nba_2010.plot.scatter(x = 'forecast', y='point_diff', c='Blue')"
   ]
